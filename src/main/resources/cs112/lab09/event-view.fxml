<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cs112.lab09.controllers.EventController">
   <bottom>
      <Button fx:id="closeButton" mnemonicParsing="false" onAction="#onCloseButtonClicked" text="CLose" BorderPane.alignment="CENTER" />
   </bottom>
   <right>
      <BorderPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <top>
            <VBox alignment="CENTER" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="locationLabel" contentDisplay="TOP" text="Location" />
                  <Label fx:id="dateLabel" text="Date" />
               </children></VBox>
         </top>
         <center>
            <VBox alignment="CENTER" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="descriptionLabel" text="" textAlignment="CENTER" wrapText="true" />
                  <Label fx:id="revisedDescription" text="ReviewReviewReviewReviewReviewReviewReviewReview" textAlignment="CENTER" wrapText="true" />
               </children></VBox>
         </center>
      </BorderPane>
   </right>
</BorderPane>
